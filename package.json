  "license": "MIT",
  "dependencies": {
    "reveal-md": "^5.3.4",
    "webpack": "^5.75.0"
  },
  "scripts": {
    "compile": "function wrapper () { marp $(find $1 -name '*.md' -type f); }; wrapper",
    "newcompile": "function wrapper () { marp -I $1 -o $2; }; wrapper",
    "marp": "marp -I slides -o docs",
    "Jeff_publish": "rm -rf docs; marp -I slides -o docs; git add -A; git commit -m 'new'; git push"
  },
  "devDependencies": {
    "@marp-team/marp-cli": "^2.2.0",
    "webpack-cli": "^4.10.0"
  }
}
